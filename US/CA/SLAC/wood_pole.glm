#meta library Name="Wood Poles"
#meta library Version="0.0"
#meta library Author="dchassin"
#meta library Description="Wood poles library for California USA"
#meta library Module="powerflow"

module powerflow;

object pole_configuration 
{
	name "WOOD-C-45/5";
	pole_type WOOD;
	pole_length 45 ft;
	pole_depth 4.5 ft;
	ground_diameter (32.5/3.14);
	top_diameter (19/3.14);
	fiber_strength 8000 psi;
	repair_time 1 h;
}
